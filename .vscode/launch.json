{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Extension",
            "type": "extensionHost",
            "request": "launch",
            "args": [
                "--extensionDevelopmentPath=${workspaceFolder}/clients/vscode",
                "--disable-extensions"
            ],
            "outFiles": [
                "${workspaceFolder}/clients/vscode/out/**/*.js"
            ],
            "preLaunchTask": "build and compile"
        }
    ]
}

